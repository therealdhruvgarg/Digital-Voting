generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String   @id @default(cuid())
  aadhaarNumber String   @unique
  facialData    String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}